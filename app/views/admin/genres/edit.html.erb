<h1>ジャンル編集</h1>

 <%= form_with model:@genre, url:edit_admin_genre_path, local:true do |f| %>
  <div class="form-group">
      <label for="book_title">ジャンル名</label>
    <%= f.text_field :name, class: 'form-control book_title' %>
  </div>
  <div class="form-group">
    <input type="submit" name="commit" value="Update Book" class="btn btn-success" data-disable-with="Update Book">
  </div>
  <% end %>