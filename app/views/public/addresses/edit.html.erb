<h3>配送先編集</h3>
<table>
  <%= form_with model:@address, local:true do |f| %>
  <div class="form-group">
    郵便番号(ハイフンなし)<%= f.text_field :postal_code %>
  </div class="form-group">

  <div class="form-group">
    住所<%= f.text_field :address %>
  </div class="form-group">
  <div class="form-group">
  宛名<%= f.text_field :name %>

  <%= f.submit '変更内容を保存', class: 'btn btn-success'%>

  <% end %>
</table>