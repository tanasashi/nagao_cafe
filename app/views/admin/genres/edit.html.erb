<h1>ジャンル編集</h1>

 <%= form_with model:@genre, url:admin_genre_path, local:true do |f| %>
  <div class="form-group">
      <label for="book_title">ジャンル名</label>
    <%= f.text_field :name, class: 'form-control book_title' %>
  </div>
  <div class="form-group">
   <%= f.submit '変更を保存' %>
  </div>
  <% end %>